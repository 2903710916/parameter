{
  "name": "parameter",
  "version": "1.0.0",
  "description": "A parameter verify tools.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --check-leaks -R spec -t 1000 test/*.test.js",
    "test-cov": "node node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- --check-leaks -t 1000 test/*.test.js",
    "test-travis": "node node_modules/.bin/istanbul cover node_modules/.bin/_mocha --report lcovonly -- --check-leaks -t 1000 test/*.test.js",
    "jshint": "jshint .",
    "autod": "autod -w --prefix '~'",
